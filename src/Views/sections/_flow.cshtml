<div class="col-md-8">
    <section class="section flow" ng-controller="FlowCtrl as Flow">
        <div class="jumbotron">
            <h1>Build your bike</h1>
            <p>Pick a bike part from each section, choose your colors, and preview the result!</p>
        </div>
        <div part-section="name" data-part-type="section.partType" data-parts="section.parts" ng-repeat="(name, section) in Flow.partSections">
        </div>
    </section>
</div>
